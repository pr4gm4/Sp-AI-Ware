mont:worm
    mont:description "worm is a type of malware that can spread from an infected machine to a network. It can spread without human interacion so it could be very dangerous. generally worm exploit a vulnerability in operating system or in a buggy program";
    mont:exploit mont:software_vulnerability;
    mont:exploit mont:operating_system_vulnerability;
    mont:prevention "Keep OS up to date";
    mont:prevention mont:antimalware.

mont:iloveyou
    mont:istance_of mont:malware;
    mont:name "ILOVEYOU"; 
    mont:attack_type mont:email_phishing;
    mont:year_of_discovery "2000";
    mont:year_of_creation "2000";
    mont:subtype mont:worm;
    mont:description "This malware is sent via email with attachment iloveyou.txt.vbs. If aj user clicks on the attachment, the worm is activated and various files on the machine are deleted. Iloveyou copies the email address book of the infected machine and then sends itself to other machines.";
    mont:operating_system mont:Windows2000;
    mont:num_infected_machines "1000000000";
    mont:patch mont:uknown;
    mont:programming_language mont:VisualBasicScript;
    mont:economic_damage "7000000000$";
    mont:infection_vectors "email";
    mont:hash_identifier mont:uknown. #TODO place a hash value





